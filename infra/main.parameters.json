{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "dev"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "secondaryLocation": {
      "value": "${AZURE_SECONDARY_LOCATION}"
    },
    "useWebStagingSlot": {
      "value": true
    },
    "sqlAdminLogin": {
      "value": "${AZURE_SQL_ADMIN_LOGIN}"
    },
    "sqlAdminPassword": {
      "value": "${AZURE_SQL_ADMIN_PASSWORD}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "failedOrdersMailingList": {
      "value": "${AZURE_FAILED_ORDERS_MAILING_LIST}"
    }
  }
}